    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/eslint": "^9.6.0",
    "@unpic/svelte": "^1.0.0",
    "@vitest/coverage-istanbul": "^2.1.5",
    "@vitest/coverage-v8": "^2.1.5",
    "eslint": "^9.7.0",
